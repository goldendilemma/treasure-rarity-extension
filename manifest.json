{
  "name": "Trove Rarity",
  "description": "Check rarity score from treasure.tools directly on Trove Marketplace",
  "version": "0.1",
  "manifest_version": 3,
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://trove.treasure.lol/collection/*"],
      "js": ["src/extension/injectTrove.cs.js"]
    }
  ]
}